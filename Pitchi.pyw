def import_list():
    duplicate_count = -1
    compat_files = [('Pitchi List','*.lst')]
    i_list = askopenfilename(initialdir=words_l_dir,filetypes=compat_files)
    if i_list != '':
        read_i_list = configparser.ConfigParser()
        read_i_list.read(i_list,encoding='utf-8')
        i_list_words = read_i_list.sections()
        read_m_list = configparser.ConfigParser()
        read_m_list.read(active_list,encoding='utf-8')
        m_list_words = read_m_list.sections()
        # Remove duplicate entries
        for i in m_list_words:
            try:
                i_list_words.remove(i)
                duplicate_count += 1
            except ValueError:
                pass
        i_list_write = configparser.ConfigParser()
        j = 0
        for i in i_list_words:
            i_list_write.add_section(i_list_words[j])
            i_list_write.set(i_list_words[j],'mora',read_i_list.get(i_list_words[j],'mora'))
            i_list_write.set(i_list_words[j],'pap',read_i_list.get(i_list_words[j],'pap'))
            i_list_write.set(i_list_words[j],'romaji',read_i_list.get(i_list_words[j],'romaji'))
            i_list_write.set(i_list_words[j],'translation',read_i_list.get(i_list_words[j],'translation'))
            i_list_write.set(i_list_words[j],'notes',read_i_list.get(i_list_words[j],'notes'))
            print('Writing ' + i_list_words[j] + ' to master list...')
            j += 1
        m_list_file = open(active_list,'a',encoding='utf-8')
        i_list_write.write(m_list_file)
        m_list_file.close()
        # Show Success
        tkMessageBox.showinfo('List imported!',message='List imported succesfully! \nWords added: ' + str(j) + '\nWords already present: '+ str(duplicate_count))
        recount_words(False)
        word_count_label.configure(text=str(word_count) + ' words loaded ')
    else:
        print('Bur! Why did you not open file?')
