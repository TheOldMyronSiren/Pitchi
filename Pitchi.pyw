def recount_words(show_message):
    global word_count
    words_l_file_o = configparser.ConfigParser()
    words_l_file_o.read(active_list,encoding='utf-8')
    section_list = words_l_file_o.sections()
    written_word_count = int(words_l_file_o.get('Info','word count'))
    # Compare word counts
    if len(section_list)-1 != written_word_count:
        word_count = len(section_list)-1
        if show_message == True:
            if word_count - written_word_count > 0:
                tkMessageBox.showinfo(title='New words detected!',message='It looks like there have been ' + str(word_count - written_word_count) + ' words added to your local list.')
            elif word_count - written_word_count < 0:
                tkMessageBox.showinfo(title='Words missing!',message='It looks like there have been ' + str(abs(word_count - written_word_count)) + ' words removed from your local list.')
        words_l_file_o.set('Info','word count',str(word_count))
        words_l_file = open(active_list,'w',encoding='utf-8')
        words_l_file_o.write(words_l_file)
        words_l_file.close()
    else:
        print('We got the same word count, bur!')
        word_count = len(section_list)-1
