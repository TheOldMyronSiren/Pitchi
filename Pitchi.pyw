def confirm_add(word,romaji,trans,mora,pap,notes,notify):
    # Write word to lst file
    if check_word_exists(word) == 'free':
        if word == '':
            tkMessageBox.showerror('Error',message='You must enter a word!')
            add_word_window.focus()
        elif romaji == '' or trans == '' or mora == '' or pap == '':
            if tkMessageBox.askquestion(title='Confirm entry',message='Some of the required fields are empty! \nAre you sure you want to add this word?') == True:
                words_l_file_o = configparser.ConfigParser()
                words_l_file_o.add_section(str(word))
                words_l_file_o.set(word,'mora',mora)
                words_l_file_o.set(word,'pap',pap)
                words_l_file_o.set(word,'romaji',romaji)
                words_l_file_o.set(word,'translation',trans)
                words_l_file_o.set(word,'notes',notes)
                words_l_file = open(active_list,'a',encoding='utf-8')
                words_l_file_o.write(words_l_file)
                words_l_file.close()
                notify.configure(text=str(word) + ' added to list!')
                add_word_window.after(3000,lambda: notify.configure(text=''))
                count_update_file = configparser.ConfigParser()
                count_update_file.read(active_list,encoding='utf-8')
                count_update_file.set('Info','word count',str(int(count_update_file.get('Info','word count'))+1))
                count_update = open(active_list,'w',encoding='utf-8')
                count_update_file.write(count_update)
                count_update.close()
            else:
                print('Good call, bur.')
                add_word_window.focus()
        else:
            words_l_file_o = configparser.ConfigParser()
            words_l_file_o.add_section(str(word))
            words_l_file_o.set(word,'mora',mora)
            words_l_file_o.set(word,'pap',pap)
            words_l_file_o.set(word,'romaji',romaji)
            words_l_file_o.set(word,'translation',trans)
            words_l_file_o.set(word,'notes',notes)
            words_l_file = open(active_list,'a',encoding='utf-8')
            words_l_file_o.write(words_l_file)
            words_l_file.close()
            notify.configure(text=str(word) + ' added to list!')
            add_word_window.after(3000,lambda: notify.configure(text=''))
            count_update_file = configparser.ConfigParser()
            count_update_file.read(active_list,encoding='utf-8')
            count_update_file.set('Info','word count',str(int(count_update_file.get('Info','word count'))+1))
            count_update = open(active_list,'w',encoding='utf-8')
            count_update_file.write(count_update)
            count_update.close()
    else:
        tkMessageBox.showerror(title='Error',message='It looks like ' + word + ' already exists in the list!')
        add_word_window.focus()
